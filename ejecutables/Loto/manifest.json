{
  "manifest_version": 2,


  "name": "Loto: complement for game",
  "short_name": "LotoGame",
  "version": "1.2.3",
  "description": "Lanzar alertas para el juego.",
  "author": "Laura y Gisella",


  "browser_action": {
    "default_popup": "home.html",
    
    "default_icon": {
      "19": "icon/toolbar-icon19.png",
      "38": "icon/toolbar-icon38.png"
    }
  },
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },


  "background": {
    "scripts": ["index.js"]
  },

  "permissions": [
    "tabs",
    "<all_urls>",
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [
    "resources/overlay.html"
  ]
}
